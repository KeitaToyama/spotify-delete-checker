datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Track {
  id         String   @id @default(uuid())
  name       String
  artist     String[] //アーティスト名(複数可)
  album      String //アルバム名
  url        String //spotify状のurl
  playlistId String
  isPlayable Boolean
  createdAt  DateTime @default(now()) // 作成日時
  updatedAt  DateTime @updatedAt // 更新日時
}
